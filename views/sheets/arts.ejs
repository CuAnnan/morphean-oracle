<div class="container">
    <div class="row">
        <h3 class="col text-center">Cantrips</h3>
    </div>
    <% for (let art of arts){%>
        <div class="row">
            <h4 class="col text-center"><%- art.name %></h4>
        </div>
        <% for (let cantrip of art.cantrips){%>
            <div class="row">
                <div class="col-2"><strong><%- cantrip.name %></strong></div>
                <div class="col"><%- cantrip.effect %></div>
            </div>
        <%}%>
    <%}%>
</div>